# corefs configuration
[Distribution]
Distribution=fedora
Release=34

[Output]
Bootable=yes
BootProtocols=uefi
Format=gpt_btrfs
Hostname=corefs_2108
Output=corefs_2108.img
OutputDir=mkosi.output/corefs/2108


[Packages]
Cache=../__sharedCache__
ExtraTrees=mkosi.extra/corefs
Packages=
	btrfs-progs
	dmidecode
	htop
	openssh-server
	passwd
	procps-ng
	shadow-utils
	systemd-networkd
	usbutils
	zstd

[Partitions]
RootSize=2G
SwapSize=2G

[Validation]
BMap=yes
CheckSum=yes
Sign=no
Key=boxos_signature
Password=changeme


